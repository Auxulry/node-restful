{
   "name": "node-restful",
   "version": "1.0.0",
   "description": "Node JS Restful",
   "author": "Mochamad Akbar",
   "license": "MIT",
   "main": "src/server.ts",
   "scripts": {
      "start": "node dist/server.js",
      "dev": "nodemon ./src/server.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migrate:create": "yarn run typeorm migration:create",
      "migrate:run": "yarn run typeorm migration:run -d src/interfaces/database/database.provider.ts",
      "migrate:revert": "yarn run typeorm migration:revert -d src/interfaces/database/database.provider.ts",
      "build": "tsc",
      "install:clean": "rm -rf node_modules/ && yarn install",
      "test": "mocha ./src/tests/*.spec.ts --exit",
      "test:watch": "mocha --watch ./src/tests/*.spec.ts ",
      "lint": "eslint ./src/** --ext .ts -c .eslintrc.json",
      "lint:fix": "yarn run lint --fix"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.4.0",
      "@types/jsonwebtoken": "^9.0.1",
      "@types/node": "^16.11.10",
      "@types/swagger-jsdoc": "^6.0.1",
      "@types/swagger-ui-express": "^4.1.3",
      "@typescript-eslint/eslint-plugin": "^5.55.0",
      "@typescript-eslint/parser": "^5.55.0",
      "eslint": "^8.36.0",
      "jest": "^29.5.0",
      "nodemon": "^2.0.21",
      "rimraf": "^4.4.0",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^4.6.2",
      "ts-jest": "^29.0.5",
      "ts-node": "10.7.0",
      "ts-node-dev": "^2.0.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "bcrypt": "^5.1.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "helmet": "^6.0.1",
      "jsonwebtoken": "^9.0.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "tsyringe": "^4.7.0",
      "typeorm": "0.3.12"
   }
}
